{
  "name": "angular-todo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:report": "ng test && istanbul-combine -d coverage/final -r lcov -r html coverage/**/coverage-final.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch mock-data.json",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "serve:todo-header": "ng serve --project=todo-header",
    "test:todo-header": "ng test --project=todo-header",
    "build:todo-header:elements": "ng build todo-header --prod --output-hashing none && node elements-build/elements-build.todo-header.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.2",
    "@angular/common": "~8.0.2",
    "@angular/compiler": "~8.0.2",
    "@angular/core": "~8.0.2",
    "@angular/elements": "^8.0.2",
    "@angular/forms": "~8.0.2",
    "@angular/platform-browser": "~8.0.2",
    "@angular/platform-browser-dynamic": "~8.0.2",
    "@angular/router": "~8.0.2",
    "@ng-bootstrap/ng-bootstrap": "^4.1.3",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "document-register-element": "^1.13.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.2",
    "@angular/language-service": "~8.0.2",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.10",
    "codelyzer": "^5.0.1",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "fs-extra": "^8.0.1",
    "istanbul-combine": "^0.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.0",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5"
  }
}
